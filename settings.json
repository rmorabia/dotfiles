{
  "editor.accessibilitySupport": "off",
  "files.autoSave": "afterDelay",
  "editor.fontSize": 14,
  "editor.fontFamily": "\"Fragment Mono\", Menlo, Monaco, 'Courier New', monospace",
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.gotoLocation.alternativeDeclarationCommand": "",
  "editor.gotoLocation.alternativeDefinitionCommand": "",
  "editor.gotoLocation.alternativeTypeDefinitionCommand": "",
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.lightbulb.enabled": "onCode",
  "editor.linkedEditing": true,
  "editor.tabCompletion": "on",
  "editor.cursorBlinking": "phase",
  "editor.formatOnPaste": true,
  "diffEditor.codeLens": true,
  "diffEditor.experimental.showMoves": true,
  "editor.minimap.autohide": "mouseover",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.suggest.preview": true,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "window.closeWhenEmpty": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.tabSize": 2,
  "editor.fontLigatures": true,
  "terminal.integrated.scrollback": 20000,
  "terminal.integrated.lineHeight": 1.3,
  "workbench.sideBar.location": "right",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "git.confirmSync": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/**.snap": true,
    "package-lock.json": true,
  },
  "window.commandCenter": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.layoutControl.enabled": false,
  "editor.minimap.enabled": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "workbench.editor.showTabs": "none",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "emmet.excludeLanguages": [
    "typescriptreact",
    "javascriptreact",
    "javascript",
    "typescript"
  ],
  "debug.terminal.clearBeforeReusing": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.suggest.enabled": true,
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "githubPullRequests.pullBranch": "never",
  "[nunjucks]": {
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "git.ignoreRebaseWarning": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.env.osx": {},
  "workbench.tree.renderIndentGuides": "always",
  "git.enableSmartCommit": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.tabs.enabled": false,
  "workbench.secondarySideBar.showLabels": false,
  "workbench.colorTheme": "Nushu Light",
  "terminal.integrated.defaultProfile.osx": "fish",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}
